{
  "manifest_version": 2,

  "name": "Clairvoyant",
  "description": "Removes secrets, you didn't even see",
  "version": "0.1",

  "background": {
    "scripts": ["exif.js"]
  },

  "permissions": ["<all_urls>"],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentscript.js"]
    }
  ]
}
